{
  "task": {
    "task_type": "regression",
    "dataset": "Hook3D-simple",
    "function_set": ["add", "sub", "mul", "const"],
    "metric": "inv_nrmse", 
    "metric_params": [1.0],
    "extra_metric_test": null,
    "extra_metric_test_params": [],
    "threshold": 1e-12,
    "protected": false,
    "reward_noise": 0.0,
    "reward_noise_type": "r",
    "normalize_variance": false,
    "decision_tree_threshold_set": []
  },

  "training": {
    "n_samples": 10000,
    "batch_size": 100,
    "epsilon": 0.02,
    "n_cores_batch": 1,
    "early_stopping": true,
    "verbose": true
  },

  "logging": {
    "save_summary": true,
    "save_all_iterations": true,
    "hof": 10
  },

  "policy": {
    "policy_type": "rnn",
    "max_length": 15
  },

  "policy_optimizer": {
    "policy_optimizer_type": "pg",
    "learning_rate": 0.005,
    "entropy_weight": 0.02,
    "entropy_gamma": 0.8
  },

  "experiment": {
    "seed": 0,
    "save_path": "results/hook3d_fast"
  },

  "state_manager": {
    "type": "hierarchical",
    "observe_parent": true,
    "observe_sibling": true,
    "observe_action": false,
    "observe_dangling": false,
    "embedding": false,
    "embedding_size": 8
  },

  "checkpoint": {
    "save_freq": null,
    "save_on_done": false
  },

  "prior": {
    "length": {
      "min_": 4,
      "max_": 12,
      "on": true
    },
    "repeat": {
      "tokens": "const",
      "min_": null,
      "max_": 3,
      "on": true
    },
    "inverse": {
      "on": true
    },
    "trig": {
      "on": false
    },
    "const": {
      "on": true
    },
    "no_inputs": {
      "on": false
    },
    "uniform_arity": {
      "on": true
    },
    "soft_length": {
      "loc": 6,
      "scale": 3,
      "on": true
    }
  }
}