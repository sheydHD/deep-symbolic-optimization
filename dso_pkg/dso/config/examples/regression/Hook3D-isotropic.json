{
  "task": {
    "task_type": "regression",
    "dataset": "Hook3D-isotropic",
    "function_set": ["add", "sub", "mul"],
    "metric": "inv_nrmse",
    "metric_params": [1.0],
    "extra_metric_test": null,
    "extra_metric_test_params": [],
    "threshold": 1e-8,
    "protected": false,
    "reward_noise": 0.0,
    "reward_noise_type": "r", 
    "normalize_variance": false,
    "decision_tree_threshold_set": []
  },

  "training": {
    "n_samples": 8000000,
    "batch_size": 1000,
    "epsilon": 0.02,
    "n_cores_batch": 1,
    "early_stopping": true
  },

  "logging": {
    "save_summary": true
  },

  "policy_optimizer": {
    "policy_optimizer_type": "pg",
    "learning_rate": 0.0025,
    "entropy_weight": 0.03,
    "entropy_gamma": 0.7
  },

  "experiment": {
    "seed": 0,
    "save_path": "results/hook3d_isotropic"
  },

  "state_manager": {
    "type": "hierarchical",
    "observe_parent": true,
    "observe_sibling": true,
    "observe_action": false,
    "observe_dangling": false,
    "embedding": false,
    "embedding_size": 8
  },

  "prior": {
    "length": {
      "min_": 4,
      "max_": 64,
      "on": true
    },
    "repeat": {
      "tokens": "const",
      "min_": null,
      "max_": 3,
      "on": true
    },
    "inverse": {
      "on": true
    },
    "trig": {
      "on": true
    },
    "const": {
      "on": true
    },
    "no_inputs": {
      "on": true
    },
    "uniform_arity": {
      "on": true
    },
    "soft_length": {
      "loc": 10,
      "scale": 5,
      "on": true
    },
    "domain_range": {
      "on": false
    }
  }
}